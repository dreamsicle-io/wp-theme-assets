{
	"type": "module",
	"name": "wp-theme",
	"themeName": "WP Theme",
	"version": "0.0.1",
	"description": "Just another WordPress theme.",
	"repository": {
		"type": "git",
		"url": "git+https://git.example.org/wp-theme.git"
	},
	"keywords": [
		"accessibility-ready",
		"translation-ready"
	],
	"author": {
		"name": "Example Org",
		"email": "info@example.org",
		"url": "https://www.example.org"
	},
	"contributors": [],
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://git.example.org/wp-theme/issues"
	},
	"homepage": "https://www.example.org",
	"wordpress": {
		"versionRequired": "6.4.0",
		"versionTested": "6.4.0"
	},
	"browserslist": {
		"development": [
			"last 1 versions"
		],
		"production": [
			"last 5 versions"
		]
	},
	"dependencies": {
		"normalize.css": "~8.0.1"
	},
	"devDependencies": {
		"@wordpress/scripts": "~27.4.0",
		"dir-archiver": "~2.1.0",
		"webpack-remove-empty-scripts": "~1.0.4"
	},
	"scripts": {
		"prepare": "composer install",
		"start": "wp-scripts start --webpack-no-externals --webpack-src-dir='.'",
		"build": "wp-scripts build --webpack-no-externals --webpack-src-dir='.'",
		"lint": "composer run lint && wp-scripts lint-style && wp-scripts lint-js",
		"fix": "composer run fix && wp-scripts lint-style --fix && wp-scripts lint-js --fix",
	}
}
