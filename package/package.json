{
	"name": "wp-theme",
	"themeName": "WP Theme",
	"version": "0.0.1",
	"description": "Just another WordPress theme.",
	"repository": {
		"type": "git",
		"url": "git+https://git.example.org/wp-theme.git"
	},
	"keywords": [
		"accessibility-ready",
		"translation-ready"
	],
	"author": {
		"name": "Example Org",
		"email": "info@example.org",
		"url": "https://www.example.org"
	},
	"contributors": [],
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://git.example.org/wp-theme/issues"
	},
	"homepage": "https://www.example.org",
	"wordpress": {
		"versionRequired": "6.4.0",
		"versionTested": "6.4.0"
	},
	"browserslist": {
		"development": [
			"last 1 versions"
		],
		"production": [
			"last 5 versions"
		]
	},
	"devDependencies": {
		"@wordpress/scripts": "~27.4.0",
		"webpack-remove-empty-scripts": "~1.0.4"
	},
	"scripts": {
		"prepare": "node scripts/prepare.mjs",
		"start": "wp-scripts start --webpack-no-externals",
		"build": "wp-scripts build --webpack-no-externals",
		"build:package": "node scripts/build-package.mjs",
		"b": "npm run build:package && npm run build",
		"lint:styles": "wp-scripts lint-style",
		"lint:scripts": "wp-scripts lint-js",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"format": "wp-scripts format"
	},
	"dependencies": {
		"normalize.css": "~8.0.1"
	}
}
