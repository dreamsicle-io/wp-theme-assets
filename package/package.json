{
	"type": "module",
	"name": "wp-theme",
	"themeName": "WP Theme",
	"version": "0.0.1",
	"description": "Just another WordPress theme.",
	"repository": {
		"type": "git",
		"url": "git+https://git.example.org/wp-theme.git"
	},
	"keywords": [
		"accessibility-ready",
		"translation-ready"
	],
	"author": {
		"name": "Example Org",
		"email": "info@example.org",
		"url": "https://www.example.org"
	},
	"contributors": [],
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://git.example.org/wp-theme/issues"
	},
	"homepage": "https://www.example.org",
	"wordpress": {
		"versionRequired": "5.0.0",
		"versionTested": "5.0.0"
