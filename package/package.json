	"dependencies": {
		
	},
	"devDependencies": {
		"@babel/core": "~7.16.7",
		"@babel/preset-env": "~7.16.8",
		"@ronilaukkarinen/gulp-stylelint": "~14.1.1",
		"babelify": "~10.0.0",
		"browserify": "~17.0.0",
		"del": "~6.0.0",
		"gulp": "~4.0.2",
		"gulp-autoprefixer": "~8.0.0",
		"gulp-cached": "~1.1.1",
		"gulp-concat": "~2.6.1",
		"gulp-debug": "~4.0.0",
		"gulp-eslint": "~6.0.0",
		"gulp-if": "~3.0.0",
		"gulp-imagemin": "~8.0.0",
		"gulp-order": "~1.2.0",
		"gulp-phpcs": "~3.1.0",
		"gulp-rename": "~2.0.0",
		"gulp-sass": "~5.1.0",
		"gulp-sourcemaps": "~3.0.0",
		"gulp-tap": "~2.0.0",
		"gulp-uglify": "~3.0.2",
		"gulp-wp-pot": "~2.5.0",
